        "@rollup/plugin-node-resolve": "^15.2.1",
        "@types/node": "^20.8.2",
        "prettier": "^3.0.3",
        "rollup": "^3.29.4",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "undici": "^5.25.4"
