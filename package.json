        "type": "git",
        "url": "https://github.com/AuthArmor/autharmor-node.git"
    },
    "files": [
        "./dist"
    ],
    "scripts": {
        "build": "rollup --config",
        "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\"",
